{
  "extends": "tslint:latest",
  "rules": {
    "no-unused-expression": false,
    "variable-name": [
      true,
      "allow-leading-underscore"
    ],
    "whitespace": [
      true,
      "check-operator",
      "check-branch",
      "check-decl",
      "check-module",
      "check-separator",
      "check-type"
    ]
  }
}