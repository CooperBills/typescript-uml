{
  "name": "typescript-uml",
  "version": "0.1.0",
  "description": "TypeScript to UML reverse engineering",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fsahmad/typescript-uml.git"
  },
  "keywords": [
    "typescript",
    "uml",
    "delinter"
  ],
  "files": [
    "lib"
  ],
  "main": "lib/typescript-uml.js",
  "typings": "lib/typescript-uml.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "tslint --format verbose \"src/**/*.ts\" \"test/**/*.ts\"",
    "build": "npm run clean && npm run lint && echo Using TypeScript && tsc --version && tsc --pretty",
    "test": "npm run build && mocha --compilers ts:ts-node/register --recursive test",
    "watch": "npm run build -- --watch",
    "watch:test": "npm run test -- --watch",    
    "test-travis": "ts-node node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- --compilers ts:ts-node/register -R spec 'test/**/*-spec.ts'"
  },
  "dependencies": {
    "typescript": "2.0.3",
    "typescript-collections": "^1.2.3",
    "uuid": "^2.0.3"
  },
  "devDependencies": {
    "@types/chai": "3.4.30",
    "@types/mocha": "2.2.29",
    "@types/node": "6.0.31",
    "@types/sinon": "^1.16.32",
    "@types/sinon-chai": "^2.7.27",
    "@types/uuid": "^2.0.29",
    "chai": "^3.5.0",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.0.1",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "ts-node": "^0.9.3",
    "tslint": "^4.0.2"
  },
  "engines": {
    "node": ">=4.0.0"
  }
}